{
  "compileOnSave": false,
  "compilerOptions": {
    "allowJs": false,
    "resolveJsonModule": true,
    "newLine": "CRLF",
    "skipLibCheck": true,
    "module": "commonjs",
    "target": "es5",
    "lib": ["dom", "dom.iterable", "es2015", "es2015.iterable", "es2015.promise", "es7"],
    "sourceMap": true,
    "jsx": "react",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
